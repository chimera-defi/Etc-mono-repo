# Cadence Backend Environment Variables
# Copy to .env and fill in values

# Server
NODE_ENV=development
PORT=8080

# Database (Neon PostgreSQL)
DATABASE_URL=postgresql://user:password@host.neon.tech/cadence?sslmode=require

# Cache (Upstash Redis)
REDIS_URL=redis://default:password@host.upstash.io:6379

# AI Providers
ANTHROPIC_API_KEY=sk-ant-...
OPENAI_API_KEY=sk-...

# GitHub OAuth
GITHUB_CLIENT_ID=Iv1.xxxxxxxxx
GITHUB_CLIENT_SECRET=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# JWT Secret (generate with: openssl rand -hex 32)
JWT_SECRET=your-32-char-secret-here

# API Key Encryption (generate with: openssl rand -hex 32)
ENCRYPTION_KEY=your-64-char-hex-key-here

# Fly.io (for agent execution)
FLY_API_TOKEN=...

# Expo Push Notifications
EXPO_ACCESS_TOKEN=...
