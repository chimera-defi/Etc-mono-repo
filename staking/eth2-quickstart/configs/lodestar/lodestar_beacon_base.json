{
  "network": "mainnet",
  "port": 9000,
  "discoveryPort": 9000,
  "execution": {
    "urls": ["http://$LH:$ENGINE_PORT"]
  },
  "rest": {
    "enabled": true,
    "host": "$CONSENSUS_HOST",
    "cors": "*"
  },
  "metrics": {
    "enabled": true,
    "host": "$CONSENSUS_HOST",
    "port": 8008
  },
  "builder": {
    "enabled": false,
    "urls": ["http://$MEV_HOST:$MEV_PORT"]
  },
  "doppelgangerProtection": {
    "enabled": true
  },
  "logLevel": "info",
  "maxPeers": 100,
  "targetPeers": 50
}