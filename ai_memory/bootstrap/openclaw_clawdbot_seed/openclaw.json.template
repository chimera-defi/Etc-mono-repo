{
  "meta": {
    "lastTouchedVersion": "2026.2.9"
  },
  "auth": {
    "profiles": {
      "openai-codex:default": {
        "provider": "openai-codex",
        "mode": "oauth"
      }
    }
  },
  "models": {
    "mode": "merge",
    "providers": {
      "ollama": {
        "baseUrl": "http://127.0.0.1:11434/v1",
        "apiKey": "ollama-local",
        "api": "openai-completions"
      },
      "llama-server": {
        "baseUrl": "http://127.0.0.1:8081/v1",
        "apiKey": "llama-server-local",
        "api": "openai-completions",
        "models": [
          {
            "id": "qwen3.5:35b",
            "name": "Qwen3.5 35B (llama-server)"
          }
        ]
      }
    }
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "openai-codex/gpt-5.3-codex"
      },
      "workspace": "/root/clawd",
      "memorySearch": {
        "provider": "local",
        "fallback": "none",
        "local": {
          "modelPath": "hf:ggml-org/embeddinggemma-300M-GGUF/embeddinggemma-300M-Q8_0.gguf"
        }
      },
      "heartbeat": {
        "every": "24h",
        "target": "none"
      }
    },
    "list": [
      {
        "id": "main",
        "identity": {
          "name": "Clawdie1",
          "theme": "octopus-minimalist",
          "emoji": "üêô"
        }
      }
    ]
  },
  "hooks": {
    "internal": {
      "enabled": true,
      "entries": {
        "boot-md": { "enabled": true },
        "command-logger": { "enabled": true },
        "session-memory": { "enabled": true }
      }
    }
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "dmPolicy": "allowlist",
      "botToken": "${OPENCLAW_TELEGRAM_BOT_TOKEN}",
      "allowFrom": ["${OPENCLAW_TELEGRAM_ALLOWED_USER_ID}"],
      "groupPolicy": "allowlist",
      "streamMode": "partial"
    }
  },
  "gateway": {
    "port": 18789,
    "mode": "local",
    "bind": "loopback",
    "auth": {
      "mode": "token",
      "token": "${OPENCLAW_GATEWAY_TOKEN}"
    }
  }
}
