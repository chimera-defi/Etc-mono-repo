{
  "schema": "ai-memory-export/v1",
  "export_id": "openclaw-clawdbot-seed",
  "description": "Portable OpenClaw/Clawdbot persona + memory + runtime templates",
  "target_workspace_root": "/root/clawd",
  "steps": [
    { "op": "copy", "source": "AGENTS.md", "target": "AGENTS.md" },
    { "op": "copy", "source": "SOUL.md", "target": "SOUL.md" },
    { "op": "copy", "source": "IDENTITY.md", "target": "IDENTITY.md" },
    { "op": "copy", "source": "USER.md", "target": "USER.md" },
    { "op": "copy", "source": "TOOLS.md", "target": "TOOLS.md" },
    { "op": "copy", "source": "HEARTBEAT.md", "target": "HEARTBEAT.md" },
    { "op": "copy", "source": "MEMORY.md", "target": "MEMORY.md" },
    { "op": "copy", "source": "memory/2026-02-09.md", "target": "memory/2026-02-09.md" },
    { "op": "copy", "source": "memory/2026-02-11.md", "target": "memory/2026-02-11.md" },
    { "op": "copy", "source": "memory/2026-02-12.md", "target": "memory/2026-02-12.md" },
    { "op": "write_template", "source": "openclaw.json.template", "target": "/root/.openclaw/openclaw.json" },
    { "op": "write_template", "source": "clawdbot.json.template", "target": "/root/.openclaw/clawdbot.json" }
  ],
  "required_placeholders": [
    "OPENCLAW_TELEGRAM_BOT_TOKEN",
    "OPENCLAW_TELEGRAM_ALLOWED_USER_ID",
    "OPENCLAW_GATEWAY_TOKEN",
    "CLAWDBOT_TELEGRAM_BOT_TOKEN",
    "CLAWDBOT_TELEGRAM_ALLOWED_USER_ID",
    "CLAWDBOT_GATEWAY_TOKEN"
  ]
}
